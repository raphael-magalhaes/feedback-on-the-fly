{
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "lint:fix": "yarn lint --fix",
        "test": "jest",
        "coverage": "yarn coverage:collect && yarn coverage:open",
        "coverage:collect": "jest --collectCoverage",
        "coverage:open": "http-server ./coverage/lcov-report/ -c-1 -o",
        "mutation": "yarn mutation:run && yarn mutation:open",
        "mutation:run": "yarn stryker run",
        "mutation:open": "http-server ./reports/mutation/html/ -c-1 -o ",
        "eject": "expo eject"
    },
    "dependencies": {
        "expo": "~36.0.0",
        "react": "~16.9.0",
        "react-dom": "~16.9.0",
        "react-native": "https://github.com/expo/react-native/archive/sdk-36.0.0.tar.gz",
        "react-native-screens": "2.0.0-alpha.12",
        "react-native-web": "~0.11.7"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@stryker-mutator/core": "^2.5.0",
        "@stryker-mutator/html-reporter": "^2.5.0",
        "@stryker-mutator/javascript-mutator": "^2.5.0",
        "@stryker-mutator/jest-runner": "^2.5.0",
        "@stryker-mutator/typescript": "^2.5.0",
        "@types/jest": "^25.1.2",
        "@types/react": "~16.9.0",
        "@types/react-native": "~0.60.23",
        "@typescript-eslint/eslint-plugin": "^2.23.0",
        "@typescript-eslint/parser": "^2.23.0",
        "babel-preset-expo": "~8.0.0",
        "eslint": "^6.8.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-react": "^7.19.0",
        "http-server": "^0.12.1",
        "jest-expo": "^36.0.1",
        "prettier": "^1.19.1",
        "react-test-renderer": "^16.12.0",
        "typescript": "~3.6.3"
    },
    "private": true
}
